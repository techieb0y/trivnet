[Unit]
Description=trivnet async jobs
After=network.target
After=postgresql-13.service
Wants=postgresql-13.service

[Service]
ExecStart=/bin/cd /var/www/trivnet && /opt/rh/rh-php73/root/bin/php /var/www/trivnet/async.php --daemon
Restart=always
Type=simple
User=trivnet

[Install]
WantedBy=multi-user.target
